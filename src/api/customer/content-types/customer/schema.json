{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "password": {
      "type": "password",
      "required": true
    },
    "registrationDate": {
      "type": "date",
      "default": "2022-04-30",
      "required": true
    },
    "phone": {
      "type": "string"
    },
    "address": {
      "type": "component",
      "repeatable": false,
      "component": "address.address"
    }
  }
}
